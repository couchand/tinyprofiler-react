{
  "name": "tinyprofiler-react",
  "version": "0.0.1",
  "description": "ReactJS front-end for TinyProfiler",
  "author": "Andrew Couch <tinyprofiler@andrewcou.ch>",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "coffee -c -o lib src",
    "deploy": "npm run clean && npm run build && npm run package",
    "prepackage": "mkdir dist",
    "package": "cp -r lib README.markdown LICENSE package.json dist",
    "clean": "rm -rf lib dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/couchand/tinyprofiler-react.git"
  },
  "bugs": {
    "url": "https://github.com/couchand/tinyprofiler-react/issues"
  },
  "keywords": [
    "tinyprofiler",
    "ui"
  ],
  "dependencies": {
    "react": "^15",
    "react-dom": "^15"
  }
}
